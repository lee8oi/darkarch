gtk-alternative-button-order = 1

style "default"
{
  bg[NORMAL] = { 0.063, 0.063, 0.063 }
  bg[SELECTED] = { 0.000, 0.192, 0.431 }
  bg[INSENSITIVE] = { 0.063, 0.063, 0.063 }
  bg[ACTIVE] = { 0.424, 0.424, 0.424 }
  bg[PRELIGHT] = { 0.063, 0.063, 0.063 }

  base[NORMAL] = { 0.000, 0.000, 0.000 }
  base[SELECTED] = { 0.000, 0.192, 0.431 }
  base[INSENSITIVE] = { 0.063, 0.063, 0.063 }
  base[ACTIVE] = { 0.000, 0.192, 0.431 }
  base[PRELIGHT] = { 0.000, 0.192, 0.431 }

  text[NORMAL] = { 1.000, 1.000, 1.000 }
  text[SELECTED] = { 1.000, 1.000, 1.000 }
  text[INSENSITIVE] = { 0.424, 0.424, 0.424 }
  text[ACTIVE] = { 1.000, 1.000, 1.000 }
  text[PRELIGHT] = { 1.000, 1.000, 1.000 }

  fg[NORMAL] = { 1.000, 1.000, 1.000 }
  fg[SELECTED] = { 1.000, 1.000, 1.000 }
  fg[INSENSITIVE] = { 0.424, 0.424, 0.424 }
  fg[ACTIVE] = { 1.000, 1.000, 1.000 }
  fg[PRELIGHT] = { 1.000, 1.000, 1.000 }
}

class "*" style "default"

style "ToolTip"
{
  bg[NORMAL] = { 0.063, 0.063, 0.063 }
  base[NORMAL] = { 0.000, 0.000, 0.000 }
  text[NORMAL] = { 1.000, 1.000, 1.000 }
  fg[NORMAL] = { 1.000, 1.000, 1.000 }
}

widget "gtk-tooltip" style "ToolTip"
widget "gtk-tooltips" style "ToolTip"

style "MenuItem"
{
  bg[PRELIGHT] = { 0.000, 0.192, 0.431 }
  fg[PRELIGHT] = { 1.000, 1.000, 1.000 }
}

class "*MenuItem" style "MenuItem"

